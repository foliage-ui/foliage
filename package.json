{
  "name": "foliage",
  "version": "0.105.0",
  "description": "Styled Components for forest",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://foliage.dev",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "lint": "eslint ./",
    "build": "tsc --build ./tsconfig.build.json && webpack",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "start": "parcel example/index.html --no-cache",
    "prepublishOnly": "rm -rf dist && yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/sergeysova/foliage"
  },
  "files": ["dist"],
  "keywords": [
    "components",
    "dom",
    "effector",
    "foliage",
    "forest",
    "style",
    "styled"
  ],
  "author": "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com/)",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@eslint-kit/eslint-config-base": "^2.1.2",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-prettier": "^2.0.0",
    "@eslint-kit/eslint-config-typescript": "^3.1.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@typescript-eslint/parser": "^4.4.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "effector": "^21.7.5",
    "eslint": "^7.11.0",
    "forest": "^0.19.6",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "sharec-sova-config": "^0.1.0",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^4.1.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "sharec": {
    "config": "sharec-sova-config",
    "version": "0.1.1"
  },
  "dependencies": {
    "stylis": "^4.0.0"
  },
  "peerDependencies": {
    "effector": "^21.7.5",
    "forest": "^0.19.6"
  }
}
